@model IEnumerable<Shop.Models.Product>

@{
    ViewData["Title"] = "Index";
}

<h1>Католог</h1>

<table class="table">
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Balance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td> 
        </tr>
}
    </tbody>
</table>
